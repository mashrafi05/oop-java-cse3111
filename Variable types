public class simple {

	public static void main(String[] args) {
		
		int num = 10;
		double dec = 20.4;
		char chr = 'A';
		String str = "Rabbi";
		boolean bool = true;
		
		System.out.println("This is integer: "+num);
		System.out.println("this is float: "+dec);
		System.out.println("This is character: "+chr);
		System.out.println("This is string: "+str);
		System.out.println("This is boolean: "+bool);
		

	}

}
